<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models
  http://axelor.com/xml/ns/domain-models/domain-models_5.4.xsd">

  <module name="student" package="com.axelor.student.db" />
  
    <entity name="Project" cachable="true">
   	    <string name="name" unique="true" required="true"/>
   	    <string name="description" translatable="true" multiline="true"/>
   	    <string name="notes" multiline="true" />
   	    <integer name="priority" />
		<one-to-many ref="ProjectTask" name="tasks" mappedBy="project"/>
   		<many-to-many ref="Contact" name="contact"/>
		
  </entity>
  
</domain-models>

