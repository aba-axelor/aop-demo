<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://axelor.com/xml/ns/object-views
  http://axelor.com/xml/ns/object-views/object-views_5.4.xsd">

	<grid name="qualification-grid" title="Qualification"
		model="com.axelor.student.db.Qualification">
		<field name="hsc" />
		<field name="hscGpa" />
		<field name="ssc" />
		<field name="sscGpa" />
		<field name="technology"/>
	</grid>

	<form name="qualification-form" title="Qualification"
		model="com.axelor.student.db.Qualification" onNew="gpa.show">
	<panel>
		<field name="hsc" onChange="gpa.show"/>
		<field name="hscGpa" />
		<field name="ssc" onChange="gpa.show"/>
		<field name="sscGpa" />
		<field name="technology"/>
	</panel>
	</form>
	
	<action-attrs name="gpa.show">
		<attribute name="readonly" for="hscGpa" expr="!hsc"/>
		<attribute name="readonly" for="sscGpa" expr="!ssc"/>
	</action-attrs>
	
	

</object-views>